{
  "name": "js-tether",
  "version": "0.1.0",
  "description": "tether tool of js",
  "keywords": [
    "common",
    "js",
    "tool"
  ],
  "homepage": "https://github.com/pefish/js-tether",
  "bugs": "https://github.com/pefish/js-tether/issues",
  "license": "MIT",
  "author": "pefish",
  "engines": {
    "node": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pefish/js-tether"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "test": "mocha -t 20000 -R spec --compilers js:babel-core/register --require babel-polyfill tests/*.test.js"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "ignore": [
      "node_modules"
    ],
    "env": {
      "build": {
        "optional": [
          "optimisation",
          "minification"
        ]
      }
    }
  },
  "dependencies": {
    "bigi": "^1.4.2",
    "bitcoin-core": "^2.0.0",
    "js-btc": "^0.2.4",
    "js-node-assist": "^0.5.6",
    "p-js-error": "^0.0.4"
  }
}
